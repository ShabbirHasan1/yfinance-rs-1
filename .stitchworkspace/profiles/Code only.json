{
  "name": "Code only",
  "settings": {
    "version": 1,
    "ext_filter": "-.md",
    "exclude_dirs": ".git, target, fixtures, .idea, tests, examples, .github",
    "exclude_files": "Cargo.lock, LICENSE",
    "remove_prefix": "",
    "remove_regex": "",
    "hierarchy_only": false,
    "dirs_only": false
  },
  "explicit": [
    {
      "path": "CODE_OF_CONDUCT.md",
      "state": true
    },
    {
      "path": "",
      "state": true
    },
    {
      "path": "CONTRIBUTING.md",
      "state": true
    }
  ]
}