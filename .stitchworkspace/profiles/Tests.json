{
  "name": "Tests",
  "settings": {
    "version": 1,
    "ext_filter": ".rs, .toml",
    "exclude_dirs": ".git, target, fixtures, .idea",
    "exclude_files": "LICENSE, Cargo.lock, package-lock.json, yarn.lock, .DS_Store, .dockerignore, .gitignore, .npmignore, .pre-commit-config.yaml, .prettierignore, .prettierrc, eslint.config.js, .env, Thumbs.db",
    "remove_prefix": "//",
    "remove_regex": "(?s)/\\*.*?\\*/ ",
    "hierarchy_only": false,
    "dirs_only": false
  },
  "explicit": [
    {
      "path": "examples",
      "state": false
    },
    {
      "path": "README.md",
      "state": false
    },
    {
      "path": "src",
      "state": false
    },
    {
      "path": "tests",
      "state": true
    },
    {
      "path": "",
      "state": true
    }
  ]
}